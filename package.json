{
  "name": "ember-data-autocomplete-js",
  "version": "1.0.7",
  "description": "Ember Data driven autocomplete.js text input.",
  "keywords": [
    "ember-addon",
    "autocomplete.js",
    "autocomplete",
    "typeahead",
    "algolia",
    "cybertooth.io"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cybertooth-io/ember-data-autocomplete-js"
  },
  "license": "MIT",
  "author": {
    "name": "cybertooth.io",
    "email": "dev@cybertooth.io",
    "url": "http://cybertooth.io"
  },
  "contributors": [
    {
      "name": "Dan Nelson",
      "email": "dan@cybertooth.io",
      "url": "https://nadnoslen.github.io/"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "fix": "npm-run-all --aggregate-output --continue-on-error --parallel fix:*",
    "fix:hbs": "ember-template-lint --fix .",
    "fix:js": "eslint --fix .",
    "format": "npm-run-all fix prettier",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prettier": "prettier --write '**/*.{css,hbs,html,js,json,md,ts,yaml,yml}'",
    "start": "ember serve",
    "test": "npm-run-all lint test:ember",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "autocomplete.js-cybertooth.io": "^0.36.0-cybertooth.1",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-data-storefront": "^0.18.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "ember-angle-bracket-invocation-polyfill": "^3.0.0",
    "ember-cli": "~3.28.3",
    "ember-cli-addon-docs": "^0.8.0",
    "ember-cli-addon-docs-esdoc": "^0.2.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "^3.24.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-template-lint-plugin-prettier": "^2.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://cybertooth-io.github.io/ember-data-autocomplete-js/"
  },
  "homepage": "https://cybertooth-io.github.io/ember-data-autocomplete-js"
}
