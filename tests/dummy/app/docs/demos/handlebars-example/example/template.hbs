<DocsDemo as |demo|>
  {{#demo.example name="handlebars-invocation-template"}}
    {{autocomplete
            autofocus="true"
            class="docs-shadow docs-appearance-none docs-border docs-rounded docs-w-full docs-py-2 docs-px-3 docs-text-gray-700 docs-leading-tight focus:docs-outline-none focus:docs-shadow-outline"
            placeholder="Handlebars Invocation Of Component - Begin Typing"
            displayKey="fullName"
            filter="by_name_search"
            modelName="user"
            selected=(fn this.chooseUser)
    }}
  {{/demo.example}}

  {{demo.snippet "handlebars-invocation-template" label="template.hbs"}}
  {{demo.snippet "basic-controller" label="controller.js"}}
  {{demo.snippet "user.js" label="user.js"}}
</DocsDemo>
